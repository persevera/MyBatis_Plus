<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.liuyong.mybatisplus.Daointerface.UserMapper">
    <insert id="insertSelective">
        insert into User
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">uid,</if>
            <if test="name != null">name,</if>
            <if test="age != null">age,</if>
            <if test="email != null">email,</if>
            <if test="if_delete != null">if___delete,</if>
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="name != null">#{name,jdbcType=VARCHAR},</if>
            <if test="age != null">#{age,jdbcType=NUMERIC},</if>
            <if test="email != null">#{email,jdbcType=VARCHAR},</if>
            <if test="if_delete != null">#{if_delete,jdbcType=NUMERIC},</if>
        </trim>
    </insert>

</mapper>